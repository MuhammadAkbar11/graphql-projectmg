{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./server/build",
    "baseUrl": "./server",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "@data/*": [
        "data/*"
      ],
      "@middleware/*": [
        "src/middleware/*"
      ],
      "@schema/*": [
        "src/schema/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@controller/*": [
        "src/controller/*"
      ],
      "@model/*": [
        "src/models/*"
      ],
      "@service/*": [
        "src/services/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@appRoute": [
        "src/routers/index.router.ts"
      ],
      "@router/*": [
        "src/routers/*"
      ]
    }
  },
}